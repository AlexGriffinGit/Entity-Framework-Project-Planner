    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\AddSymbol.png" />
    <None Remove="Images\CrossSymbol.png" />
    <None Remove="Images\DeleteSymbol.png" />
