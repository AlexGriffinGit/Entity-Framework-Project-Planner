    <Resource Include="Images\CrossSymbol.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\DeleteSymbol.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\ModifySymbol.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\TickSymbol.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
